{
  "type": "module",
  "name": "kysely-generate",
  "version": "0.20.0",
  "author": "VastBlast",
  "license": "MIT",
  "bin": {
    "kysely-generate": "./dist/cli/bin.mjs"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/VastBlast/kysely-generate.git"
  },
  "bugs": {
    "url": "https://github.com/VastBlast/kysely-generate/issues"
  },
  "homepage": "https://github.com/VastBlast/kysely-generate#readme",
  "scripts": {
    "build": "tsdown",
    "dev": "tsdown --watch",
    "docker:up": "docker-compose up -d",
    "check:types": "tsgo --noEmit",
    "lint": "oxlint --type-aware",
    "lint:fix": "oxlint --fix --type-aware",
    "check": "npm run lint && npm run check:types",
    "prepublishOnly": "npm run build && npm run check && npm run test",
    "start": "npm run build && DATABASE_URL=postgres://user:password@localhost:5433/database node ./dist/cli/bin.js",
    "test": "vitest run --globals --fileParallelism=false --maxConcurrency=1 --maxWorkers=1 --sequence.seed=1",
    "test:watch": "vitest watch --globals --fileParallelism=false --maxConcurrency=1 --maxWorkers=1 --sequence.seed=1"
  },
  "dependencies": {
    "chalk": "4.1.2",
    "cosmiconfig": "^9.0.0",
    "dotenv": "^17.2.1",
    "dotenv-expand": "^12.0.2",
    "git-diff": "^2.0.6",
    "micromatch": "^4.0.8",
    "minimist": "^1.2.8",
    "pluralize": "^8.0.0",
    "zod": "^4.1.5"
  },
  "devDependencies": {
    "@libsql/kysely-libsql": "^0.4.1",
    "@tediousjs/connection-string": "^0.5.0",
    "@types/better-sqlite3": "^7.6.13",
    "@types/bun": "^1.2.21",
    "@types/git-diff": "^2.0.7",
    "@types/micromatch": "^4.0.9",
    "@types/minimist": "^1.2.5",
    "@types/node": "^24.3.0",
    "@types/pg": "^8.15.5",
    "@types/pluralize": "^0.0.33",
    "@typescript/native-preview": "^7.0.0-dev.20251218.3",
    "better-sqlite3": "^12.2.0",
    "execa": "^9.6.0",
    "kysely": "^0.28.5",
    "kysely-bun-sqlite": "^0.4.0",
    "kysely-bun-worker": "^1.2.1",
    "mysql2": "^3.14.4",
    "oxlint": "^1.33.0",
    "oxlint-tsgolint": "^0.9.2",
    "pg": "^8.16.3",
    "postgres-interval": "^4.0.2",
    "prettier": "^3.6.2",
    "tarn": "^3.0.2",
    "tedious": "^18.6.1",
    "ts-dedent": "^2.2.0",
    "tsdown": "^0.18.1",
    "typescript": "^5.9.3",
    "vitest": "^3.2.4"
  },
  "peerDependencies": {
    "@libsql/kysely-libsql": ">=0.3.0 <0.5.0",
    "@tediousjs/connection-string": ">=0.5.0 <0.6.0",
    "better-sqlite3": ">=7.6.2 <13.0.0",
    "kysely": ">=0.27.0 <1.0.0",
    "kysely-bun-sqlite": ">=0.3.2 <1.0.0",
    "kysely-bun-worker": ">=1.2.0 <2.0.0",
    "mysql2": ">=2.3.3 <4.0.0",
    "pg": ">=8.8.0 <9.0.0",
    "tarn": ">=3.0.0 <4.0.0",
    "tedious": ">=18.0.0 <20.0.0"
  },
  "peerDependenciesMeta": {
    "@libsql/kysely-libsql": {
      "optional": true
    },
    "@tediousjs/connection-string": {
      "optional": true
    },
    "better-sqlite3": {
      "optional": true
    },
    "kysely": {
      "optional": false
    },
    "kysely-bun-sqlite": {
      "optional": true
    },
    "kysely-bun-worker": {
      "optional": true
    },
    "mysql2": {
      "optional": true
    },
    "pg": {
      "optional": true
    },
    "tarn": {
      "optional": true
    },
    "tedious": {
      "optional": true
    }
  },
  "engines": {
    "node": ">=20.0.0"
  },
  "main": "./dist/index.cjs",
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.cts",
  "exports": {
    ".": {
      "require": "./dist/index.cjs",
      "import": "./dist/index.mjs"
    },
    "./cli/bin": {
      "require": "./dist/cli/bin.cjs",
      "import": "./dist/cli/bin.mjs"
    },
    "./package.json": "./package.json"
  }
}
