{
  "name": "kysely-codegen",
  "version": "0.17.1",
  "author": "Robin Blomberg",
  "license": "MIT",
  "main": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "bin": {
    "kysely-codegen": "./dist/cli/bin.js"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/RobinBlomberg/kysely-codegen.git"
  },
  "bugs": {
    "url": "https://github.com/RobinBlomberg/kysely-codegen/issues"
  },
  "homepage": "https://github.com/RobinBlomberg/kysely-codegen#readme",
  "scripts": {
    "build": "rimraf dist && tsc --project ./tsconfig.build.json",
    "check:types": "tsc --noEmit",
    "ci": "run-s ci:*",
    "ci:build": "pnpm build",
    "ci:eslint": "pnpm lint:eslint --max-warnings=0 --report-unused-disable-directives",
    "ci:prettier": "pnpm lint:prettier",
    "ci:spelling": "cspell-cli -e dist -e pnpm-lock.yaml -e '*.svg' .",
    "ci:test": "pnpm test",
    "ci:unused": "knip",
    "dev": "tsx watch ./src/cli/bin.ts",
    "docker:up": "docker-compose up -d",
    "fix": "run-s fix:*",
    "fix:eslint": "eslint --fix src",
    "fix:prettier": "prettier --write src",
    "lint": "run-p lint:*",
    "lint:eslint": "eslint src",
    "lint:prettier": "prettier --check src",
    "prepublishOnly": "pnpm run ci",
    "start": "pnpm build && node ./dist/cli/bin.js",
    "test": "vitest run --globals",
    "test:watch": "vitest watch --globals",
    "upgrade": "ncu -u"
  },
  "dependencies": {
    "chalk": "4.1.2",
    "cosmiconfig": "^9.0.0",
    "dotenv": "^16.4.7",
    "dotenv-expand": "^12.0.1",
    "git-diff": "^2.0.6",
    "micromatch": "^4.0.8",
    "minimist": "^1.2.8",
    "pluralize": "^8.0.0"
  },
  "devDependencies": {
    "@libsql/kysely-libsql": "^0.4.1",
    "@robinblomberg/eslint-config-prettier": "^0.1.4",
    "@robinblomberg/eslint-config-robinblomberg": "0.28.0",
    "@robinblomberg/prettier-config": "^0.1.2",
    "@types/better-sqlite3": "^7.6.12",
    "@types/bun": "^1.2.4",
    "@types/git-diff": "^2.0.7",
    "@types/micromatch": "^4.0.9",
    "@types/minimist": "^1.2.5",
    "@types/node": "^22.13.8",
    "@types/pg": "^8.11.11",
    "@types/pluralize": "^0.0.33",
    "@types/tedious": "^18.0.0",
    "better-sqlite3": "^11.8.1",
    "cspell-cli": "^8.17.3",
    "eslint": "^8.57.0",
    "execa": "^9.5.2",
    "knip": "^5.45.0",
    "kysely": "^0.27.5",
    "mysql2": "^3.12.0",
    "npm-run-all": "^4.1.5",
    "pg": "^8.13.3",
    "postgres-interval": "^4.0.2",
    "prettier": "^3.5.2",
    "rimraf": "^6.0.1",
    "ts-dedent": "^2.2.0",
    "tsx": "^4.19.3",
    "typescript": "^5.8.2",
    "vitest": "^3.0.7",
    "zod": "^3.24.2"
  },
  "peerDependencies": {
    "@libsql/kysely-libsql": "^0.3.0",
    "@tediousjs/connection-string": "^0.5.0",
    "better-sqlite3": ">=7.6.2",
    "kysely": "^0.27.0",
    "kysely-bun-sqlite": "^0.3.2",
    "kysely-bun-worker": "^0.5.3",
    "mysql2": "^2.3.3 || ^3.0.0",
    "pg": "^8.8.0",
    "tarn": "^3.0.0",
    "tedious": "^18.0.0"
  },
  "peerDependenciesMeta": {
    "@libsql/kysely-libsql": {
      "optional": true
    },
    "@tediousjs/connection-string": {
      "optional": true
    },
    "better-sqlite3": {
      "optional": true
    },
    "kysely": {
      "optional": false
    },
    "kysely-bun-sqlite": {
      "optional": true
    },
    "kysely-bun-worker": {
      "optional": true
    },
    "mysql2": {
      "optional": true
    },
    "pg": {
      "optional": true
    },
    "tarn": {
      "optional": true
    },
    "tedious": {
      "optional": true
    }
  },
  "eslintConfig": {
    "extends": [
      "@robinblomberg/robinblomberg",
      "@robinblomberg/prettier"
    ],
    "ignorePatterns": "**/*.snapshot.ts",
    "rules": {
      "@typescript-eslint/consistent-type-imports": [
        1,
        {
          "disallowTypeAnnotations": false,
          "fixStyle": "separate-type-imports",
          "prefer": "type-imports"
        }
      ],
      "unicorn/prefer-node-protocol": 0
    }
  },
  "knip": {
    "ignore": [
      "src/db.ts",
      "**/*.snapshot.ts"
    ],
    "ignoreBinaries": [
      "docker-compose",
      "ncu"
    ],
    "ignoreDependencies": [
      "@libsql/kysely-libsql",
      "@tediousjs/connection-string",
      "better-sqlite3",
      "mysql2",
      "pg",
      "tarn",
      "tedious"
    ]
  },
  "prettier": "@robinblomberg/prettier-config"
}
